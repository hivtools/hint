    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "ag-grid-community": "^32.1.0",
    "ag-grid-vue3": "^32.1.0",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.26.18",
    "chart.js": "^4.4.7",
    "chartjs-plugin-annotation": "^3.1.0",
    "d3-scale-chromatic": "^3.1.0",
    "feather-icons": "^4.29.2",
    "floating-vue": "^5.2.2",
    "geojson": "^0.5.0",
    "i18next": "^24.2.0",
    "jsonata": "^1.8.7",
    "katex": "^0.16.19",
    "leaflet": "^1.9.4",
    "numeral": "^2.0.6",
    "plotly.js-basic-dist": "^2.35.3",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-feather": "^2.0.0",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
