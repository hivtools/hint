{
  "name": "hint",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-with-check": "run-p type-check \"build {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:integration": "vitest --config test-integration-configs/integration.mts --run --coverage",
    "test:adr-integration": "vitest --config test-integration-configs/adr-integration.ts --run",
    "build": "vite build",
    "type-check": "vue-tsc -p tsconfig.app.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "copy-build-files": "rm -rf public && mkdir -p public && cp -r ./dist/. ./public/",
    "copy-assets-to-public": "rm -rf public && mkdir -p public && cp -r ./assets/. ./public/"
  },
  "license": "MIT",
  "web-types": "./web-types.json",
  "dependencies": {
    "@reside-ic/vue-nested-multiselect": "^0.0.15",
